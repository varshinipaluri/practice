import java.util.*;
public class Solution {
    public static int linearSearch(int n, int num, int []arr){
        // Write your code here.
        int x=0;
        for(int i=0;i<n;i++){
            if(arr[i]==num){
                x=i;
                break;
            }
            else x=-1;
        }
        return x;
    }
}
