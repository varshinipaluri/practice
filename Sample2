import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        String[] s1=s.split(":");
        String str="";
        for(int i=0;i<s1.length;i++){
            if(s1[i].charAt(0)==s1[i].charAt(1)){
                str+=s1[i].charAt(0);
            }
        }
        System.out.println(str.toUpperCase());
    }
}
