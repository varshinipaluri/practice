import java.util.*;
public class Sample{
    public static void main(String args[]){
        Scanner sc=new Scanner (System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
         int sum=0,m=1;
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            int x=arr[i]/m;
            sum+=(x*x);
            m=m*10;
        }
       System.out.println(sum);
        
    }
}
